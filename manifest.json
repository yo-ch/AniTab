{
  "manifest_version": 2,

  "name": "NewTab!",
  "description": "Displays the current time with a wallpaper in new tabs.",
  "version": "1.0",

  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },

  "permissions": ["storage"],

  "icons": {
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
